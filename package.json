{
  "name": "catch-site-starter",
  "version": "1.0.0",
  "private": true,
  "packageManager": "bun@1.3.5",
  "scripts": {
    "generate-icons": "node scripts/generate-pwa-icons.js",
    "verify-icons": "node scripts/verify-pwa-icons.js",
    "dev": "next dev --turbopack",
    "dev:warm": "next dev --turbopack & sleep 4 && bun run warmup && wait",
    "warmup": "tsx scripts/warmup-routes.ts",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "test": "bun run test:unit && bun run test:integration && bun run test:e2e",
    "test:unit": "vitest run docs/TESTING/the-catch-testing-kit/tests/unit",
    "test:unit:watch": "vitest docs/TESTING/the-catch-testing-kit/tests/unit",
    "test:integration": "vitest run docs/TESTING/the-catch-testing-kit/tests/integration",
    "test:e2e": "playwright test --config docs/TESTING/the-catch-testing-kit/playwright.config.ts",
    "test:e2e:ui": "playwright test --config docs/TESTING/the-catch-testing-kit/playwright.config.ts --ui",
    "test:e2e:debug": "PWDEBUG=1 playwright test --config docs/TESTING/the-catch-testing-kit/playwright.config.ts",
    "test:a11y": "playwright test --config docs/TESTING/the-catch-testing-kit/playwright.config.ts docs/TESTING/the-catch-testing-kit/tests/e2e/accessibility.spec.ts",
    "vercel:check": "tsx scripts/check-vercel-config.ts",
    "stripe:setup": "tsx scripts/ecommerce/setup-stripe-locations.ts",
    "stripe:check": "tsx scripts/ecommerce/check-stripe-status.ts",
    "locations:migrate": "tsx scripts/ecommerce/migrate-locations.ts"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.36.10",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@react-email/components": "^1.0.6",
    "@react-pdf/renderer": "^4.3.2",
    "@sanity/client": "^7.14.1",
    "@sanity/image-url": "^2.0.3",
    "@sanity/ui": "^3.1.11",
    "@sanity/vision": "^5.7.0",
    "@sentry/nextjs": "^10.37.0",
    "@silk-hq/components": "^0.10.0",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "groq": "^5.7.0",
    "lucide-react": "^0.563.0",
    "mapbox-gl": "^3.18.1",
    "mixitup": "^3.3.2",
    "next": "^16.1.6",
    "next-sanity": "^12.0.14",
    "next-themes": "^0.4.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "resend": "^6.9.1",
    "sanity": "^5.7.0",
    "sharp": "^0.34.5",
    "styled-components": "6.3.8",
    "tailwind-merge": "^3.4.0",
    "twilio": "^5.12.0",
    "zod": "^4.3.6"
  },
  "overrides": {
    "date-fns": "2.30.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@eslint/compat": "^2.0.1",
    "@eslint/eslintrc": "^3.3.3",
    "@next/bundle-analyzer": "^16.1.6",
    "@next/eslint-plugin-next": "^16.1.6",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitejs/plugin-react": "^5.1.2",
    "baseline-browser-mapping": "^2.9.18",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^27.4.0",
    "playwright": "^1.58.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite-tsconfig-paths": "^6.0.5",
    "vitest": "^4.0.18"
  }
}
