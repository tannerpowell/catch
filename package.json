{
  "name": "catch-site-starter",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.26.1",
  "scripts": {
    "generate-icons": "node scripts/generate-pwa-icons.js",
    "verify-icons": "node scripts/verify-pwa-icons.js",
    "generate:theme": "tsx scripts/generate-theme-css.ts",
    "predev": "pnpm run generate:theme",
    "dev": "next dev --turbopack",
    "predev:warm": "pnpm run generate:theme",
    "dev:warm": "next dev --turbopack & sleep 4 && pnpm warmup && wait",
    "warmup": "tsx scripts/warmup-routes.ts",
    "prebuild": "pnpm run generate:theme",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "test": "pnpm run test:unit && pnpm run test:integration && pnpm run test:e2e",
    "test:unit": "vitest run docs/TESTING/the-catch-testing-kit/tests/unit",
    "test:unit:watch": "vitest docs/TESTING/the-catch-testing-kit/tests/unit",
    "test:integration": "vitest run docs/TESTING/the-catch-testing-kit/tests/integration",
    "test:e2e": "playwright test --config docs/TESTING/the-catch-testing-kit/playwright.config.ts",
    "test:e2e:ui": "playwright test --config docs/TESTING/the-catch-testing-kit/playwright.config.ts --ui",
    "test:e2e:debug": "PWDEBUG=1 playwright test --config docs/TESTING/the-catch-testing-kit/playwright.config.ts",
    "test:a11y": "playwright test --config docs/TESTING/the-catch-testing-kit/playwright.config.ts docs/TESTING/the-catch-testing-kit/tests/e2e/accessibility.spec.ts",
    "vercel:check": "tsx scripts/check-vercel-config.ts",
    "stripe:setup": "tsx scripts/ecommerce/setup-stripe-locations.ts",
    "stripe:check": "tsx scripts/ecommerce/check-stripe-status.ts",
    "locations:migrate": "tsx scripts/ecommerce/migrate-locations.ts",
    "verify:contrast": "tsx scripts/verify-color-contrast.ts",
    "find:colors": "tsx scripts/find-hardcoded-colors.ts"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.36.7",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@react-email/components": "^1.0.4",
    "@react-pdf/renderer": "^4.3.2",
    "@sanity/client": "^7.14.0",
    "@sanity/image-url": "^2.0.2",
    "@sanity/ui": "^3.1.11",
    "@sanity/vision": "^5.3.1",
    "@sentry/nextjs": "^10.34.0",
    "@silk-hq/components": "^0.10.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "groq": "^5.3.1",
    "lucide-react": "^0.562.0",
    "mapbox-gl": "^3.18.0",
    "mixitup": "^3.3.2",
    "next": "^16.1.2",
    "next-sanity": "^12.0.12",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "resend": "^6.7.0",
    "sanity": "^5.3.1",
    "sharp": "^0.34.4",
    "styled-components": "^6.3.6",
    "tailwind-merge": "^3.4.0",
    "twilio": "^5.11.2",
    "zod": "^4.3.5"
  },
  "overrides": {
    "date-fns": "2.30.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/compat": "^2.0.1",
    "@eslint/eslintrc": "^3.3.1",
    "@next/bundle-analyzer": "^16.1.2",
    "@next/eslint-plugin-next": "^16.1.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.1.0",
    "@types/node": "^25.0.8",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitejs/plugin-react": "^5.1.2",
    "baseline-browser-mapping": "^2.9.14",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.1.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^27.2.0",
    "playwright": "^1.55.1",
    "postcss": "^8.4.37",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.53.0",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^4.0.17"
  }
}
